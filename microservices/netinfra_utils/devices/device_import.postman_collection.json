{
	"info": {
		"_postman_id": "bee6da9d-d4c7-43c0-933d-5c3038af33b5",
		"name": "Default device import",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "cli",
			"item": [
				{
					"name": "Import devices",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"device_type\": \"{{device_type}}\",\n\t\"device_version\": \"{{device_version}}\",\n\t\"host\": \"sample-topology\",\n\t\"id\": \"{{device_id}}\",\n\t\"password\": \"{{password}}\",\n\t\"username\": \"{{username}}\",\n\t\"port\": \"{{port}}\",\n\t\"transport_type\": \"ssh\"\n\t\"labels\" : [\"{{labels}}\"]\n}"
						},
						"url": {
							"raw": "http://elasticsearch:9200/inventory-device/device/{{device_id}}",
							"protocol": "http",
							"host": [
								"elasticsearch"
							],
							"port": "9200",
							"path": [
								"inventory-device",
								"device",
								"{{device_id}}"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "netconf",
			"item": [
				{
					"name": "Import netconf devices",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"id\": \"{{id}}\",\n   \t\"host\": \"sample-topology\",\n    \"port\": \"{{port}}\",\n    \"keepalivedelay\": \"{{keepalivedelay}}\",\n    \"tcponly\": \"{{tcponly}}\",\n    \"username\": \"{{username}}\",\n    \"password\": \"{{password}}\"\n}"
						},
						"url": {
							"raw": "http://elasticsearch:9200/inventory-netconf/device/{{device_id}}",
							"protocol": "http",
							"host": [
								"elasticsearch"
							],
							"port": "9200",
							"path": [
								"inventory-netconf",
								"device",
								"{{device_id}}"
							]
						}
					},
					"response": []
				}
			]
		}
	]
}